                factoryClass="com.jetbrains.compose.benchmark.BenchmarkToolWindow" icon="/icons/compose.svg"/>
        <toolWindow id="Compose4" anchor="right" secondary="true"
                factoryClass="com.jetbrains.compose.benchmark.BenchmarkToolWindow" icon="/icons/compose.svg"/>
        <toolWindow id="Compose5" anchor="left" secondary="true"
